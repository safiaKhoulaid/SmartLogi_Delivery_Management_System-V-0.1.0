    <changeSet id="1762338294444-2" author="ycode">
        <addForeignKeyConstraint baseColumnNames="user_id" baseTableName="gestionnaire"
                                 constraintName="FK_GESTIONNAIRE_ON_USER" referencedColumnNames="user_id"
                                 referencedTableName="base_user"/>
    </changeSet>

</databaseChangeLog>