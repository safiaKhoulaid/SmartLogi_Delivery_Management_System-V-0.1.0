<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.31.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1765751228234-4" author="ycode">
        <dropForeignKeyConstraint baseTableName="role_permissions" constraintName="fk_role_perm_permission"/>
    </changeSet>
    <changeSet id="1765751228234-3" author="ycode">
        <addUniqueConstraint columnNames="name" constraintName="uc_permission_name" tableName="permission"/>
    </changeSet>
    <changeSet id="1765751228234-6" author="ycode">
        <dropTable cascadeConstraints="true" tableName="role_permissions"/>
    </changeSet>
    <changeSet id="1765751228234-1" author="ycode">
        <dropColumn columnName="id" tableName="permission"/>
    </changeSet>
    <changeSet id="1765751228234-2" author="ycode">
        <addColumn tableName="permission">
            <column name="id" type="VARCHAR(255)">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_permission" validateNullable="true"/>
            </column>
        </addColumn>
    </changeSet>

</databaseChangeLog>